<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Weckerapp" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/atheneos_DarkGrey</item>
        <item name="colorPrimaryDark">@color/atheneos_ContrastGrey</item>
        <item name="colorAccent">@color/atheneos_yellow</item>
        <item name="android:colorBackground">@color/atheneos_LightGrey</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.Weckerapp.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Weckerapp.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Weckerapp.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>